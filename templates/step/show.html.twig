{% extends 'base.html.twig' %}

{% block title %}Step{% endblock %}

{% block body %}
    <h1>Step</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ step.id }}</td>
            </tr>
            <tr>
                <th>Month_begin</th>
                <td>{{ step.MonthBegin }}</td>
            </tr>
            <tr>
                <th>Year_begin</th>
                <td>{{ step.YearBegin }}</td>
            </tr>
            <tr>
                <th>Month_end</th>
                <td>{{ step.monthEnd }}</td>
            </tr>
            <tr>
                <th>Year_end</th>
                <td>{{ step.yearEnd }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ step.description }}</td>
            </tr>
            <tr>
                <th>Skills</th>
                <td>{{ step.skills }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_step_index') }}">back to list</a>

    <a href="{{ path('app_step_edit', {'id': step.id}) }}">edit</a>

    {{ include('step/_delete_form.html.twig') }}
