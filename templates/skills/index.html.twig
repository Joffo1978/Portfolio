{% extends 'base.html.twig' %}

{% block title %}Skills index{% endblock %}

{% block body %}
    <h1>Skills index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Languages</th>
                <th>AIRBUS</th>
                <th>DASSAULT</th>
                <th>CALCUL_DESIGN</th>
                <th>OFFICE</th>
                <th>Langues</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for skill in skills %}
            <tr>
                <td>{{ skill.id }}</td>
                <td>{{ skill.Languages }}</td>
                <td>{{ skill.AIRBUS }}</td>
                <td>{{ skill.DASSAULT }}</td>
                <td>{{ skill.CALCULDESIGN }}</td>
                <td>{{ skill.OFFICE }}</td>
                <td>{{ skill.Langues }}</td>
                <td>
                    <a href="{{ path('app_skills_show', {'id': skill.id}) }}">show</a>
                    <a href="{{ path('app_skills_edit', {'id': skill.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="8">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_skills_new') }}">Create new</a>
{% endblock %}
